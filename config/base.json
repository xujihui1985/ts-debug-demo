{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "../built",
    "rootDir": "../src",
    "sourceMap": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "forceConsistentCasingInFileNames": true,
    "charset": "utf8",
    "alwaysStrict": true,
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true
  }
}